{
  "dashboard": {
    "id": null,
    "title": "üéµ Audio System - Performance Dashboard",
    "tags": ["audio", "performance", "agente-hotelero"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "üéØ Audio System Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"agente-api-audio\"}",
            "legendFormat": "System Status",
            "refId": "A"
          },
          {
            "expr": "rate(audio_cache_hits_total[5m]) / (rate(audio_cache_hits_total[5m]) + rate(audio_cache_misses_total[5m]))",
            "legendFormat": "Cache Hit Rate",
            "refId": "B"
          },
          {
            "expr": "avg(audio_pool_health_score)",
            "legendFormat": "Pool Health",
            "refId": "C"
          },
          {
            "expr": "rate(audio_compression_operations_total{result=\"success\"}[5m])",
            "legendFormat": "Compression Ops/sec",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "horizontal"
        }
      },
      {
        "id": 2,
        "title": "üìä Cache Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(audio_cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec - {{cache_type}}",
            "refId": "A"
          },
          {
            "expr": "rate(audio_cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec - {{cache_type}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps",
            "min": 0
          }
        }
      },
      {
        "id": 3,
        "title": "üéµ Audio Operations Latency",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(audio_cache_operation_seconds_bucket[5m]))",
            "legendFormat": "p50 - {{operation}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(audio_cache_operation_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{operation}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(audio_cache_operation_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{operation}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 4,
        "title": "üíæ Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "audio_cache_memory_bytes",
            "legendFormat": "Cache Memory Used",
            "refId": "A"
          },
          {
            "expr": "512 * 1024 * 1024",
            "legendFormat": "Cache Memory Limit",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "bytes",
            "min": 0
          }
        }
      },
      {
        "id": 5,
        "title": "üîó Connection Pool Status",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "targets": [
          {
            "expr": "audio_connection_pool_size{status=\"healthy\"}",
            "legendFormat": "Healthy - {{service_type}}",
            "refId": "A"
          },
          {
            "expr": "audio_connection_pool_size{status=\"total\"}",
            "legendFormat": "Total - {{service_type}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 6,
        "title": "üóúÔ∏è Compression Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "rate(audio_compression_operations_total{result=\"success\"}[5m])",
            "legendFormat": "Success - {{level}}",
            "refId": "A"
          },
          {
            "expr": "rate(audio_compression_operations_total{result=\"error\"}[5m])",
            "legendFormat": "Errors - {{level}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps",
            "min": 0
          }
        }
      },
      {
        "id": 7,
        "title": "üìà Compression Ratios",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "audio_compression_ratio",
            "legendFormat": "Ratio - {{level}} - {{format}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "min": 1
          }
        }
      },
      {
        "id": 8,
        "title": "‚ö†Ô∏è Error Rates",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "rate(audio_compression_operations_total{result=\"error\"}[5m]) / rate(audio_compression_operations_total[5m])",
            "legendFormat": "Error Rate - {{level}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.05},
                {"color": "red", "value": 0.1}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 9,
        "title": "üéõÔ∏è Audio System Health Score",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "avg(audio_pool_health_score)",
            "legendFormat": "Health Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.8}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 10,
        "title": "üìä Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 32},
        "targets": [
          {
            "expr": "rate(audio_cache_hits_total[5m]) / (rate(audio_cache_hits_total[5m]) + rate(audio_cache_misses_total[5m]))",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 11,
        "title": "üö® Active Alerts",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "ALERTS{component=~\"audio_.*\"}",
            "legendFormat": "",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 2}
              ]
            }
          }
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true
              },
              "renameByName": {
                "alertname": "Alert",
                "alertstate": "State",
                "severity": "Severity",
                "component": "Component"
              }
            }
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Audio Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(up{job=\"agente-api-audio\"}[1m])",
          "iconColor": "blue",
          "titleFormat": "Deployment"
        }
      ]
    },
    "variables": [
      {
        "name": "instance",
        "type": "query",
        "query": "label_values(up{job=\"agente-api-audio\"}, instance)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  }
}